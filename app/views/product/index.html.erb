<h1>Products</h1>
<p>dick</p>

<table>
  <tr>
		<th>id</th>
    <th>Type</th>
    <th>Name</th>
		<th>Price</th>
		<th>Description</th>
		<th>Status</th>
		<th>URL</th>
    <th></th>
  </tr>
	<% @products.each do |p| %>
    <tr>
      <td><%= p.id %></td>
      <td><%= p.kind %></td>
			<td><%= p.name %></td>
			<td><%= p.price %></td>
			<td><%= p.description %></td>
			<td><%= p.status %></td>
			<td><%= p.url %></td>
		</tr>
	<% end %>
</table>

<%= paginate @products %>